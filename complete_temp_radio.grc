        )
        # if an attribute with the same name as a parameter is found,
        # a callback is registered (properties work, too).
        # self.example_param = example_param
        self.state = IN_GUARD_LOW
        self.previous_item = 0
        self.low_guard_no_of_samples = low_guard_no_of_samples
        self.positive_edges_in_preamble = positive_edges_in_preamble

    def work(self, input_items, output_items):
        """example: multiply with constant"""
        input = input_items[0]
        out = output_items[0]
        
        i = 0
        while i &lt; len(input):
            self.consume(0,1)
            self.start_time += 1
            
            if item[i] &lt; 0.5:
                current_item = 0
            else:
                current_item = 1
                    
            switched = current_item != previous_item
            clockpulse = 0
            
            if self.state == IN_GUARD_LOW:
                if switched:
                    state = MEASURING_HIGH
                    self.start_time = 0
                    self.edges_detected_sofar = 1
            ##            elif self.state == IN_GUARD_HIGH:
            elif self.state == MEASURING_HIGH:
                if switched:
                    self.state == MEASURING_LOW
            elif self.state == MEASURING_LOW:
                is switched:
                    self.edges_detected_sofar += 1
                    if self.edges_detected_sofar == self.positive_edges_in_preamble:
                        self.state = PLAYING_CLOCK_HIGH
                        self.samples_per_cycle = int(self.start_time / (positive_edges_in_preamble - 1))
                    else
                        self.state = MEASURING_HIGH
            
            elif self.state == PLAYING_CLOCK_LOW:
                if self.start_time % self.samples_per_cycle == 0:
                    clockpulse = 1
                if switched:
                    self.state = PLAYING_CLOCK_HIGH
            elif self.state == PLAYING_CLOCK_HIGH:
