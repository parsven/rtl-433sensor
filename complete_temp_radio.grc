    MEASURING_HIGH = 3
    MEASURING_LOW = 4
    PLAYING_CLOCK_LOW = 5
    PLAYING_CLOCK_HIGH = 6

class blk(gr.sync_block):  # other base classes are basic_block, decim_block, interp_block
    """Embedded Python Block example - a simple multiply const"""

    def __init__(self, positive_edges_in_preamble=5 , sample_time=0.5, low_guard_no_of_samples = 1000):  # only default arguments here
        """arguments to this function show up as parameters in GRC"""
        gr.sync_block.__init__(
            self,
            name='Clock recovery',   # will show up in GRC
            in_sig=[np.float32],
