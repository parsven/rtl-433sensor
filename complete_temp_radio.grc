import numpy as np
from gnuradio import gr


class blk(gr.basic_block):  # other base classes are basic_block, decim_block, interp_block
    """Embedded Python Block example - a simple multiply const"""

    def __init__(self, threshold=0.5, sample_rate=10, max_symbol_time=10.0):  # only default arguments here
        """arguments to this function show up as parameters in GRC"""
        gr.basic_block.__init__(
            self,
            name='Run length measurement',   # will show up in GRC
            in_sig=[np.float32],
            out_sig=[np.float32]
        )
        # if an attribute with the same name as a parameter is found,
        # a callback is registered (properties work, too).
        self.threshold = threshold
        self.sample_rate = sample_rate
        self.max_symbol_time = max_symbol_time
        self.is_high = False
        self.streak_count = 0


    def general_work(self, input_items, output_items):
        """example: multiply with constant"""
        # output_items[0][:] = input_items[0] #* self.threshold

        input = input_items[0]
        out = output_items[0]

        i = 0
        while i &lt; len(input):
            self.consume(0,1)
            if(self.is_high):
                if(input[i] &lt; self.threshold):
                    self.is_high = False
                    out[0] = (self.streak_count + 1.0 )/ self.sample_rate
                    print str(out[0]) + " ms HI"
                    print str(self.streak_count) + " samples"
                    return 1
                    self.streak_count = 0
            else:
                if(input[i] &gt;= self.threshold):
                    self.is_high = True
                    out[0] = (self.streak_count + 1.0 )/ self.sample_rate
                    print str(out[0]) + "ms LO"
                    print str(self.streak_count) + " samples"
                    self.streak_count = 0
                    return 1
            i += 1
            self.streak_count += 1
        return 0
